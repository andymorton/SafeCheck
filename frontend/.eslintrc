{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/require-default-props": "off"
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}
